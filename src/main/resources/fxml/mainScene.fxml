<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.control.Button?>

<VBox fx:id="mainPane" minHeight="480.0" minWidth="853.0" prefHeight="720.0" prefWidth="1280.0" styleClass="mainPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.ViewGui.MainSceneController">


    <stylesheets>
        <URL value="@../css/main.css" />
    </stylesheets>
   <children>
   
       <ImageView id="logo.png" fitHeight="151.0" fitWidth="461.0" preserveRatio="true" scaleX="1.2" scaleY="1.2" scaleZ="1.2">
         <image>
            <Image url="@../img/common/logo.png" />
         </image>
         <VBox.margin>
            <Insets bottom="50.0" />
         </VBox.margin></ImageView>
   
       <Button id="gen_button" fx:id="startButton" mnemonicParsing="false" onAction="#onStartButtonClick" prefHeight="30.0" prefWidth="170.0" scaleX="1.5" scaleY="1.5" scaleZ="1.5" stylesheets="@../css/main.css" text="Start" />
   
       <Button id="gen_button" fx:id="exitButton" mnemonicParsing="false" onAction="#onExitButtonClick" prefHeight="30.0" prefWidth="170.0" scaleX="1.5" scaleY="1.5" stylesheets="@../css/main.css" text="Exit">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin></Button>
   </children>
</VBox>
